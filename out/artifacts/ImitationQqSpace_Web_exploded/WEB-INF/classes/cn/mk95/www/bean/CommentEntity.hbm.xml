<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.mk95.www.bean.CommentEntity" table="comment" schema="easyspace">
        <id name="commentid">
            <column name="commentid" sql-type="int(11)"/>
        </id>
        <property name="noteid">
            <column name="noteid" sql-type="int(11)"/>
        </property>
        <property name="commenttime">
            <column name="commenttime" sql-type="datetime"/>
        </property>
        <property name="userid">
            <column name="userid" sql-type="int(6)"/>
        </property>
        <property name="commenturl">
            <column name="commenturl" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>