<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.mk95.www.bean.FriendEntity" table="friend" schema="easyspace">
        <id name="userid">
            <column name="userid" sql-type="int(6)"/>
        </id>
        <property name="fidurl">
            <column name="fidurl" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>