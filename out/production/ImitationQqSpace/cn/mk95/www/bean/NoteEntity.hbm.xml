<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.mk95.www.bean.NoteEntity" table="note" schema="easyspace">
        <composite-id mapped="true" class="cn.mk95.www.bean.NoteEntityPK">
            <key-property name="userid" column="userid"/>
            <key-property name="notetime" column="notetime"/>
            <key-property name="id" column=" id"/>
        </composite-id>
        <property name="noteurl" column="noteurl"/>
        <property name="like" column="like"/>
        <property name="commentid" column="commentid"/>
        <property name="noteid" column="noteid"/>
    </class>
</hibernate-mapping>